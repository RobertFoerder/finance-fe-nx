      "prefix": "finance-fe",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/finance-api"],
          "options": {
            "jestConfig": "libs/finance-api/jest.config.js",
            "passWithNoTests": true
          }
