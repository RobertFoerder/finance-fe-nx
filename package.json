    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@nrwl/angular": "13.3.1",
    "@stoplight/prism-cli": "^4.6.1",
    "ngx-toastr": "^14.2.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.0",
